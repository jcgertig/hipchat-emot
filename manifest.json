{
    "name": "HipChat Emot",
    "version": "0.0.1",
    "manifest_version": 2,
    "permissions": [
        "http://*.hipchat.com/*",
        "https://*.hipchat.com/*"
    ],
    "content_scripts": [{
        "matches": ["http://*.hipchat.com/*", "https://*.hipchat.com/*"],
        "js": ["content.js", "js/jquery-1.10.2.min.js"],
        "css": ["styling.css"]
    }],
    "web_accessible_resources": ["input.html", "js/jquery-1.10.2.min.js"]
}